net: "yolo_tiny_deploy.prototxt"
type: "SGD"
solver_mode: GPU
device_id: 0
debug_info: false

max_iter: 100000
average_loss: 100
display: 1

test_iter: 100
test_interval: 20 
test_compute_loss: true
test_initialization: false

base_lr: 0.00005 
lr_policy: "step"
gamma: 0.5
power: 1.0
stepsize: 20000
iter_size: 200
momentum: 0.9

weight_decay: 0.0005
regularization_type: "L2"

snapshot: 3000
snapshot_prefix: "caffe_yolo_train"
snapshot_diff: false
snapshot_format: BINARYPROTO
